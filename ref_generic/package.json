		"react-dom": "^19.2.3",
		"react-router-dom": "^7.11.0",
		"react-virtuoso": "^4.18.1",
		"react-window": "^2.2.5",
		"recharts": "^3.6.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/node": "^22.14.0",
		"@vitejs/plugin-react": "^5.0.0",
		"typescript": "~5.8.2",
		"vite": "^6.4.1"
